{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "React Boilerplate",
  "engines": {
    "node": "10.12.0",
    "npm": "6.4.1"
  },
  "pre-commit": [
    "linter",
    "test"
  ],
  "scripts": {
    "start": "npm run prod",
    "dev": "npm run build:dev && npm run start-dev-server",
    "prod": "npm run build:prod && npm run start-server",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --colors",
    "build:prod": "cross-env NODE_ENV=production webpack --colors",
    "linter": "eslint src __tests__ --ignore-pattern webpack* --ext .jsx --ext .js && stylelint src/**/*.scss",
    "linter:out": "eslint src __tests__ -f html --ext .jsx --ext .js -o lint-report.html",
    "linter:fix": "eslint src __tests__ -f html --ext .jsx --ext .js -o lint-report.html --fix && stylelint src/**/*.scss --fix",
    "test": "jest --coverage --coverageReporters=text",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "start-dev-server": "cross-env NODE_ENV=development node server.js",
    "start-server": "cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.5",
    "appmetrics-dash": "^4.0.0",
    "axios": "^0.18.0",
    "bcrypt": "^3.0.2",
    "bootstrap": "^4.1.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "install": "^0.12.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "npm": "^6.4.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-css-themr": "^2.1.2",
    "react-dom": "^16.6.1",
    "react-google-login": "^3.2.1",
    "react-redux": "^5.1.0",
    "react-router-dom": "^4.3.1",
    "react-tabify": "0.1.24",
    "react-tabs": "^2.3.0",
    "reactstrap": "^6.5.0",
    "reduce-reducers": "^0.4.3",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "stylelint-order": "^1.0.0",
    "stylelint-scss": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "core-js": "^2.5.7",
    "css-loader": "^1.0.1",
    "cssnano": "^4.1.0",
    "del": "^3.0.0",
    "dotenv-webpack": "^1.5.7",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "eslint": "^5.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.2",
    "node-sass": "^4.9.3",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.6.2",
    "react-hot-loader": "^4.3.6",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "stylelint": "^9.7.1",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "url-loader": "^1.1.1",
    "webpack": "^4.23.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.10"
  }
}
